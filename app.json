{
  "expo": {
    "name": "Classic Buddy",
    "slug": "classicbuddy",
    "description": "WoW Classic AH Companion app",
    "primaryColor": "#101010",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": ".src/assets/appicons/icon.png",
    "splash": {
      "image": ".src/assets/appicons/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": ".src/assets/appicons/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.jeger774.classicbuddy"
    },
    "web": {
      "favicon": ".src/assets/appicons/favicon.png",
      "bundler": "metro"
    }
  }
}
