{
  "name": "classicbuddy",
  "main": "node_modules/expo/AppEntry.js",
  "version": "0.0.1",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx --quiet"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.3",
    "@react-native-community/masked-view": "0.1.10",
