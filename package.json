    "@react-navigation/stack": "5.14.5",
    "@types/chart.js": "^2.9.37",
    "@types/react": "^18.0.27",
    "@types/react-chartjs-2": "^2.5.7",
    "@types/react-native": "^0.71.0",
    "@types/react-navigation": "^3.4.0",
    "@types/xml2js": "^0.4.11",
    "@types/xmldom": "^0.1.31",
    "dayjs": "1.10.5",
    "eas-cli": "^3.5.2",
    "experimental-react-suspender": "^1.0.5",
    "expo": "~47.0.12",
    "expo-app-loading": "~2.1.1",
    "expo-constants": "~14.0.2",
    "expo-haptics": "~12.0.1",
    "expo-linear-gradient": "~12.0.1",
    "expo-localization": "~14.0.0",
    "expo-modules-core": "~1.0.4",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-loader-spinner": "^5.3.4",
    "react-native": "0.70.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-keyboard-aware-scroll-view": "0.9.4",
    "react-native-pager-view": "6.0.1",
    "react-native-reanimated": "~2.12.0",
    "react-native-responsive-linechart": "^5.7.1",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "^13.7.0",
    "react-native-web": "~0.18.9",
    "react-native-webview": "^11.26.1",
    "react-native-xml2js": "^1.0.3",
    "react-scripts": "^5.0.1",
    "recoil": "^0.7.6",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@react-native-community/eslint-config": "2.0.0",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.4",
    "@rnx-kit/cli": "^0.16.2",
    "@rnx-kit/metro-config": "^1.3.5",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.26",
    "@types/i18n-js": "3.8.1",
    "eslint": "7.28.0",
    "jest-expo": "45.0.0",
    "typescript": "4.3.5"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ]
  },
  "rnx-kit": {
    "bundle": {
      "treeShake": true,
      "plugins": [
        [
          "@rnx-kit/metro-plugin-duplicates-checker",
          {
            "throwOnError": false
          }
        ]
      ],
      "entryFile": "index.js",
      "targets": [
        "ios"
      ],
      "platforms": {
        "ios": {
          "bundleOutput": "ios/main.jsbundle",
          "sourcemapOutput": "ios/main.jsbundle.map",
          "assetsDest": "ios"
        }
      }
    }
  }
}
